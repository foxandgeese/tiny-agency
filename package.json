{
  "name": "tiny-agency",
  "version": "1.0.1",
  "author": "Peter Bray <peter@foxandgeese.com>",
  "description": "Super-simple Material Design starter.",
  "scripts": {
    "develop": "gatsby develop",
    "lint": "eslint src/ && prettier --check 'src/**/*.js'",
    "fix": "eslint --fix src/ && prettier --write 'src/**/*.js'",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "yarn && gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "core-js": "3.15.0",
    "gatsby": "2.30.1",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.9.0",
    "gatsby-plugin-manifest": "2.10.0",
    "gatsby-plugin-material-ui": "2.1.10",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-source-filesystem": "2.9.0",
    "gatsby-transformer-remark": "2.14.0",
    "gh-pages": "3.2.3",
    "jss": "10.6.0",
    "mdi-material-ui": "6.22.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-jss": "10.6.0",
    "react-style-tag": "2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.29.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.3.1"
  }
}
